# WalletConnect project ID — get one free at https://cloud.walletconnect.com
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Dwellir API key — get one at https://dashboard.dwellir.com
# Used for mainnet info queries (higher rate limits than public endpoints)
NEXT_PUBLIC_DWELLIR_API_KEY=your_dwellir_api_key_here

# Builder address — the address that receives builder fees
# Defaults to Dwellir's builder address if not set
NEXT_PUBLIC_BUILDER_ADDRESS=0xE94D5a0a377Afe26a461e8aAC2c7189D2006c8b9

# Builder fee in tenths-of-a-basis-point (10 = 1 bps = 0.01%)
# Defaults to 10 if not set
NEXT_PUBLIC_BUILDER_FEE=10
